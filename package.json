{
  "name": "deephand-web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "dotenv -e .env.local -- astro dev",
    "dev:cf": "wrangler pages dev dist --compatibility-date=2025-01-15",
    "build": "astro build",
    "preview": "astro preview",
    "preview:cf": "pnpm build && wrangler pages dev dist --compatibility-date=2025-01-15",
    "astro": "astro",
    "type-check": "astro check",
    "typecheck": "astro check",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "deploy": "wrangler pages deploy dist",
    "deploy:production": "pnpm build && wrangler pages deploy dist --project-name deephand-web-production",
    "deploy:preview": "pnpm build && wrangler pages deploy dist --project-name deephand-web-development",
    "cf:login": "wrangler auth login",
    "cf:logout": "wrangler auth logout",
    "security:check": "pnpm audit --audit-level=moderate"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.5.5",
    "@astrojs/react": "^4.3.0",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-three/fiber": "^8.18.0",
    "@react-three/postprocessing": "^2.19.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "astro": "^5.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "framer-motion": "^11.15.0",
    "i18next": "^25.2.1",
    "lucide-react": "^0.515.0",
    "ogl": "^1.0.11",
    "postprocessing": "^6.37.4",
    "puppeteer": "^24.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.57.0",
    "react-i18next": "^15.5.3",
    "resend": "^4.6.0",
    "three": "^0.177.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/three": "^0.177.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.29.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3",
    "wrangler": "^4.20.0"
  }
}
